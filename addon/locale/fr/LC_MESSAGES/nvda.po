#: addon\globalPlugins\update_filezilla.py:30
#, python-format
msgid "Update of %s version %s"
msgstr "Mise à jour de %s version %s"

#: addon\globalPlugins\update_filezilla.py:31
#, python-format
msgid ""
"%s version %s is available. Would you like to update now? You can view the "
"changes by clicking on the What's New button and scrolling down to Changes."
msgstr ""
"%s version %s est disponible. Souhaitez-vous mettre à jour maintenant? Vous "
"pouvez voir les changements en cliquant sur le bouton quoi de neuf et "
"descendre jusqu'à changement."

#: addon\globalPlugins\update_filezilla.py:69
msgid "No update is available."
msgstr "Aucune mise à jour n'est disponible."

#: addon\globalPlugins\update_filezilla.py:74
#, python-format
msgid "%s is enabled."
msgstr "%s est activée."

#: addon\globalPlugins\update_filezilla.py:77
#, python-format
msgid "%s is disabled."
msgstr "%s est désactivée."

#: addon\globalPlugins\update_filezilla.py:83
msgid "FileZilla: checks for module updates manually"
msgstr "FileZilla: rechercher une mise à jour du module manuellement"

#: addon\globalPlugins\update_filezilla.py:87
msgid "FileZilla: enable/disable automatic update checking"
msgstr "FileZilla: active/désactive la recherche de mise à jour automatique."

#: addon\globalPlugins\update_filezilla.py:89
msgid "Automatic update"
msgstr "La mise à jour automatique"

#. Translators: This is a label of a button appearing
